{
    "manifest_version": 2,

    "name": "Delicious Bookmark Key",
    "description": "Call delicious \"add bookmark\" native window by Ctrl+Alt+D",
    "version": "1.0.1",

    "permissions": [ "tabs" , "http://*/*", "https://*/*" ],

    "icons": {
        "128": "assets/icon-128.png",
        "16": "assets/icon-16.png",
        "48": "assets/icon-48.png"
    },

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": [
                "js/content.js"
            ]
        }
    ]
}
